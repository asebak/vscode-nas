{
    "name": "nas",
    "displayName": "nas",
    "description": "nebulas vs code plugin",
    "version": "0.0.1",
    "publisher": "ahmad",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "viewsContainers": {
            "activitybar": [
                {
                    "id": "nas",
                    "title": "Nebulas",
                    "icon": "/resources/azure.svg"
                }
            ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "nebulas": "0.4.8"
    }
}
